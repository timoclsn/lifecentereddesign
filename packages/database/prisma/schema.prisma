datasource db {
  provider             = "mysql"
  url                  = env("DATABASE_URL")
  referentialIntegrity = "prisma"
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

// Legacy
model ResourceMeta {
  id    String @id @db.VarChar(255)
  likes Int    @default(0)
}
